<body>
    <section>
        <div class="container">
            <div class="row">
                <div class="col-lg-12">
                    <nav class="navbar navbar-expand-lg navbar-light bg-light">
                        <a class="navbar-brand">

                            <img src="~/images/Logo.png" />
                        </a>
                        <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent"
                                aria-expanded="false" aria-label="Toggle navigation">
                            <span class="navbar-toggler-icon"></span>
                        </button>

                        <div class="collapse navbar-collapse" id="navbarSupportedContent">
                            <ul class="navbar-nav ml-auto">
                                <li class="nav-item">
                                    <a class="nav-link" @Html.ActionLink("Home", "Index", "Home")
                                        <span class="sr-only">(current)</span>
                                    </a>
                                </li>
                                <li class="nav-item">
                                    <a class="nav-link" @Html.ActionLink("About", "About", "Home")</a>
                                </li>
                                <li class="nav-item">
                                    <a class="nav-link" @Html.ActionLink("Contact", "Contact", "Home")</a>
                                </li>
                            </ul>
                        </div>
                    </nav>
                </div>
            </div>
        </div>
    </section>

    <div class="card">
        <div class="container" style="margin-top:80px">
            <h2 style="text-align: center; ">Staff Members Table</h2>

            <center>
                <iframe width="100%" height="400"
                        src="https://docs.google.com/spreadsheets/d/1KCZ4yc-3kfmOD6IpEPr2aNtJ7bVsfZwOxMgCO0R8V4I/edit?usp=sharing?gid=0&amp;rm=minimal&amp;range:A1:B2&amp;single=true&amp;widget=false&amp;headers=false"></iframe>
            </center>
            <br>
            <hr />
            <br>
            <h2 style="text-align: center; ">
                Staff Roles Table
            </h2>
            <center>
                <iframe width="100%" height="400"
                        src="https://docs.google.com/spreadsheets/d/1ezEmub9GFdbW1yVMaKBk0j5EpRngG5zD8sNAFvv1zWQ/edit?&amp;rm=minimal&amp;widget=false&amp;headers=false&amp;"></iframe>
            </center>
        </div>
    </div>
   
</body>